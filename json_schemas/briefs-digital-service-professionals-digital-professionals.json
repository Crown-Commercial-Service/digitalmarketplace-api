{
  "$schema": "http://json-schema.org/schema#",
  "additionalProperties": false,
  "properties": {
    "additionalTerms": {
      "minLength": 0,
      "pattern": "^$|(^(?:\\S+\\s+){0,99}\\S+$)",
      "type": "string"
    },
    "budgetRange": {
      "minLength": 0,
      "pattern": "^$|(^(?:\\S+\\s+){0,99}\\S+$)",
      "type": "string"
    },
    "contractLength": {
      "maxLength": 100,
      "minLength": 0,
      "type": "string"
    },
    "culturalFitCriteria": {
      "items": {
        "maxLength": 300,
        "pattern": "^(?:\\S+\\s+){0,29}\\S+$",
        "type": "string"
      },
      "maxItems": 20,
      "minItems": 1,
      "type": "array"
    },
    "culturalWeighting": {
      "exclusiveMaximum": false,
      "maximum": 20,
      "minimum": 5,
      "type": "integer"
    },
    "essentialRequirements": {
      "items": {
        "maxLength": 300,
        "pattern": "^(?:\\S+\\s+){0,29}\\S+$",
        "type": "string"
      },
      "maxItems": 20,
      "minItems": 1,
      "type": "array"
    },
    "evaluationType": {
      "items": {
        "enum": [
          "References",
          "Interview",
          "Scenario or test",
          "Presentation"
        ]
      },
      "maxItems": 4,
      "minItems": 0,
      "type": "array",
      "uniqueItems": true
    },
    "existingTeam": {
      "minLength": 1,
      "pattern": "^(?:\\S+\\s+){0,99}\\S+$",
      "type": "string"
    },
    "location": {
      "enum": [
        "Australian Capital Territory",
        "New South Wales",
        "Northern Territory",
        "Queensland",
        "South Australia",
        "Tasmania",
        "Victoria",
        "Western Australia",
        "Offsite"
      ]
    },
    "niceToHaveRequirements": {
      "items": {
        "maxLength": 300,
        "pattern": "^(?:\\S+\\s+){0,29}\\S+$",
        "type": "string"
      },
      "maxItems": 20,
      "minItems": 0,
      "type": "array"
    },
    "numberOfSuppliers": {
      "exclusiveMaximum": false,
      "maximum": 15,
      "minimum": 3,
      "type": "integer"
    },
    "organisation": {
      "maxLength": 100,
      "minLength": 1,
      "type": "string"
    },
    "priceWeighting": {
      "exclusiveMaximum": false,
      "maximum": 85,
      "minimum": 20,
      "type": "integer"
    },
    "questionAndAnswerSessionDetails": {
      "minLength": 0,
      "pattern": "^$|(^(?:\\S+\\s+){0,99}\\S+$)",
      "type": "string"
    },
    "requirementsLength": {
      "enum": [
        "1 week",
        "2 weeks"
      ]
    },
    "securityClearance": {
      "minLength": 0,
      "pattern": "^$|(^(?:\\S+\\s+){0,49}\\S+$)",
      "type": "string"
    },
    "sellerEmail": {
      "maxLength": 300,
      "minLength": 1,
      "type": "string"
    },
    "sellerEmailList": {
      "items": {
        "maxLength": 300,
        "pattern": "^(?:\\S+\\s+){0,9}\\S+$",
        "type": "string"
      },
      "maxItems": 15,
      "minItems": 1,
      "type": "array"
    },
    "sellerSelector": {
      "enum": [
        "allSellers",
        "someSellers",
        "oneSeller"
      ]
    },
    "specialistRole": {
      "enum": [
        "agileCoach",
        "businessAnalyst",
        "deliveryManager",
        "developer",
        "digitalTransformationAdviser",
        "ethicalHacker",
        "inclusiveDesigner",
        "interactionDesigner",
        "qualityAssurance",
        "productManager",
        "serviceDesigner",
        "technicalLeadArchitect",
        "userResearcher",
        "webDevops"
      ]
    },
    "specialistWork": {
      "minLength": 1,
      "pattern": "^(?:\\S+\\s+){0,99}\\S+$",
      "type": "string"
    },
    "startDate": {
      "maxLength": 100,
      "minLength": 1,
      "type": "string"
    },
    "summary": {
      "minLength": 1,
      "pattern": "^(?:\\S+\\s+){0,49}\\S+$",
      "type": "string"
    },
    "technicalWeighting": {
      "exclusiveMaximum": false,
      "maximum": 75,
      "minimum": 10,
      "type": "integer"
    },
    "title": {
      "maxLength": 100,
      "minLength": 1,
      "type": "string"
    },
    "workingArrangements": {
      "minLength": 1,
      "pattern": "^(?:\\S+\\s+){0,99}\\S+$",
      "type": "string"
    },
    "workplaceAddress": {
      "minLength": 1,
      "pattern": "^(?:\\S+\\s+){0,99}\\S+$",
      "type": "string"
    }
  },
  "required": [
    "culturalFitCriteria",
    "culturalWeighting",
    "essentialRequirements",
    "existingTeam",
    "location",
    "numberOfSuppliers",
    "organisation",
    "priceWeighting",
    "requirementsLength",
    "sellerEmail",
    "sellerEmailList",
    "sellerSelector",
    "specialistRole",
    "specialistWork",
    "startDate",
    "summary",
    "technicalWeighting",
    "title",
    "workingArrangements",
    "workplaceAddress"
  ],
  "title": "Digital Service Professionals Brief Schema",
  "type": "object"
}
